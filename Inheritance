import java.util.Scanner;

        class Person {
            int id;
            String name;
            public  void display_P(int id,String name)
            {
                this.id = id;
                this.name = name;
                System.out.println("The id of person is"+" "+id);
                System.out.println("The name of person is"+" "+name);
            }
        }
        class Student extends Person {
            int course,fees;
            public void display_S(int course,int fees){
             this.course = course;
             this.fees = fees;
                System.out.println("The course of student is"+" "+course);
             System.out.println("The fees of student is"+" "+fees);
    }
}
public class Info{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        Student s=new Student();
        s.display_P(10,"Bob");
       s.display_S(101,3000);

    }
}
